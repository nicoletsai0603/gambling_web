}
.foot-links2 em {
  padding: 0 2em;
  font-style: normal;
}
.footer .copyright {
  text-align: center;
  padding: 5px 0;
  background: #EAEAEA;
  color: #666;
  height: 34px;
  line-height:  24px;
}
.footer .copyright > span {
  margin: 0 15px;
}
.footer .copyright .qq {
  padding-left: 25px;
  background: url(../images/icon/qq.png) 0 center no-repeat;
}
.footer .copyright .email {
  padding-left: 25px;
  background: url(../images/icon/email.png) 0 center no-repeat;
}
.footer .copyright b {
  font-weight: bold;
}
.fix-right {
  position: fixed;
  z-index: 11;
  right: 0;
  bottom: 5%;
  width: 79px;
}
.fix-right .a1 {
  display: block;
