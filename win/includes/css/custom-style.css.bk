.btn-register,
.btn-playInfo {
  margin-left: 10px;
}
.btn-register span,
.btn-playInfo span {
  display: inline-block;
  width: 88px;
  height: 40px;
  text-align: center;
  background: #46A728;
  line-height: 40px;
  font-size: 13px;
  color: #fff;
  cursor: pointer;
}
.btn-register .icon,
.btn-playInfo .icon {
  float: left;
  width: 40px;
  height: 40px;
  background: url(../images/icon/user-plus.png) center no-repeat #57BB2D;
}
.btn-playInfo span {
  background: #944CC2;
}
.btn-playInfo .icon {
  float: left;
  width: 40px;
  height: 40px;
  background: url(../images/icon/sezi.png) center no-repeat #A660D4;
}
.btn-menu {
  border: 1px solid #ccc;
  width: 40px;
  height: 40px;
  margin-right: 10px;
  margin-left: 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border-radius: 3px;
  text-align: center;
  line-height: 38px;
  font-size: 26px;
}
.nav-bar {
  height: 43px;
  background: #CE0120;
}
.nav-bar .nav {
  flex: 1;
}
.nav-bar .nav li {
  flex: 1;
  text-align: center;
  padding-left: 2px;
  padding-right: 2px;
}
.nav-bar .nav a {
  display: block;
  line-height: 43px;
  color: #fff;
  height: 43px;
  font-size: 15px;
  padding: 0;
  transition: none;
}
.nav-bar .nav li.on,
.nav-bar .nav li:hover {
  padding-top: 5px;
}
.nav-bar .nav li.on a,
.nav-bar .nav li:hover a {
  height: 38px;
  line-height: 33px;
  background: #fff;
  color: #888888;
  border-radius: 5px 5px 0 0;
}
.btn-all {
  width: 122px;
  height: 43px;
  display: block;
  position: relative;
  text-indent: -9999px;
}
.btn-all:after {
  content: "";
  display: block;
  background: url(../images/btn-all.png) no-repeat;
  height: 47px;
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
}
.pop-lottery-box {
  position: absolute;
  width: 302px;
  left: 50%;
  margin-left: -550px;
  top: 139px;
  background: #fff;
  z-index: 10000;
  display: none;
  border: 1px solid #ECECEC;
  -webkit-box-shadow: 0 3px 5px rgba(0, 0, 0, 0.35);
  -moz-box-shadow: 0 3px 5px rgba(0, 0, 0, 0.35);
  box-shadow: 0 3px 5px rgba(0, 0, 0, 0.35);
}

.cai-list ul{
  margin: 0;
  padding: 0;
}
.cai-list li {
  float: left;
  width: 33.3333%;
  text-align: center;
  list-style:  none;
}
.cai-list h6{
  margin: 0;
  line-height: 24px;
}
.cai-list a {
  display: block;
  padding: 10px;
  color: #666;
}
.cai-list .img {
  height: 60px;
  line-height: 60px;
  vertical-align: middle;
}
.cai-list .img img {
  max-height: 60px;
  vertical-align: middle;
  line-height: 0;
}
.cai-list span {
  display: inline-block;
  background: #FF910E;
  width: 70px;
  height: 28px;
  text-align: center;
  line-height: 28px;
  color: #fff;
  position: relative;
}
.cai-list span .fa {
  margin-left: -3px;
  position: absolute;
  top: -7px;
  color: #FF910E;
  left: 50%;
}
.cai-list-links {
  padding: 14px;
}
.cai-list-links .row{
  margin: 0;
}
.cai-list-links .row div {
  line-height: 28px;
  display: inline;
}
.cai-list-links .row .label {
  width: 70px;
  height: 24px;
  text-align: center;
  position: relative;
  line-height: 24px;
  display: inline-block;
  top: 1px;
  background: #77BA49;
  color: #fff;
  margin-right: 5px;
  border-radius: 0;
  padding: 0;
  font-weight: normal;
  font-size: 100%;
}
.cai-list-links .row .label .fa {
  position: absolute;
  right: -3px;
  top: 50%;
  margin-top: -6px;
  color: #77BA49;
}
.cai-list-links .row .label2 {
  background: #7C8FEE;
}
.cai-list-links .row .label2 .fa {
  color: #7C8FEE;
}
.cai-list-links .row a {
  display: inline-block;
  margin-right: 10px;
  color: #666;
}
.cai-list a:hover,
.cai-list-links .row a:hover{
  color: #DF4333;
}
.hr {
  background: #E2E2E2;
  line-height: 1px;
  height: 1px;
  clear: both;
}
/*footer*/
#footer_template h3,
#footer_template p{
  margin: 0;
  color: #666;
}
.l {
  color: #DF4333 !important;
}
.footer {
  margin-top: 20px;
  background: #fff;
  height: auto;
}
.foot-safe {
  border-bottom: 1px solid #EAEAEA;
}
.foot-safe .row{
  margin: 0;
}
.foot-safe .row > div {
  width: 24.999%;
  float: left;
}
.foot-safe .row > div:nth-last-of-type(1) .safe-box:after {
  display: none;
}
.safe-box {
  height: 80px;
  position: relative;
  padding-left: 78px;
}
.safe-box:after {
  content: "";
  display: block;
  width: 1px;
  height: 68px;
  background: #DBDBDB;
  position: absolute;
  right: 0;
  top: 6px;
}
.safe-box h3 {
  font-size: 16px;
  font-weight: bold;
  padding: 0;
  padding-top: 20px;
  margin: 0;
  line-height: 1.5;
}
#footer_template .safe-box p {
  margin: 0;
  padding: 0;
  color: #B8B8B8;
}
.safe-box .img {
  position: absolute;
  width: 70px;
  left: 0;
  top: 0;
  line-height: 80px;
  vertical-align: middle;
  text-align: right;
}
.safe-box .img img {
  vertical-align: middle;
  line-height: 0;
}
.foot-links {
  padding: 15px 0;
}
.foot-links h3 {
  font-size: 15px;
  color: #646464;
  line-height: 40px;
  font-weight: bold;
}
.foot-code {
  width: 320px;
  float: left;
}
.foot-code th,
.foot-code td {
  vertical-align: top;
}
.foot-code th {
  padding: 8px 15px 7px;
}
.foot-code th img {
  width: 75px;
  height: 75px;
}
.foot-links-box .fflex > div {
  float: left;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0 10px;
}
.foot-links-box dl{
  margin: 0;
}
.foot-links-box .link1 dl {
  padding-left: 47px;
  background: url(../images/icon/ico-ptys.png) 0 0 no-repeat;
}
.foot-links-box .link2 dl {
  padding-left: 47px;
  background: url(../images/icon/ico-zhxg.png) 0 0 no-repeat;
}
.foot-links-box .link3 dl {
  padding-left: 47px;
  background: url(../images/icon/ico-czgc.png) 0 0 no-repeat;
}
.foot-links-box .link4 dl {
  padding-left: 47px;
  background: url(../images/icon/ico-djtk.png) 0 0 no-repeat;
}
.foot-links-box dd a {
  display: inline-block;
  margin-right: 10px;
  color: #666;
  line-height: 24px;
}
.foot-links2 {
  text-align: center;
  background: #353535;
  padding-top: 12px;
  padding-bottom: 12px;
  color: #989898;
}
.foot-links2 a {
  color: #989898;
