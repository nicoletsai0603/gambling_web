.ismobile{
  display: none;
}
.all_announcement{
  color: #646464;
  line-height: 42px;
  font-family: 'Microsoft YaHei';
  font-size: 13px;
  color: #666;
  margin: 0;
}
#header_template,#footer_template{
  font-size: 13px;
  color: #666;
  line-height: 24px;
  font-family: 'Microsoft YaHei';
  background: #EAEAEA;
}
@media only screen and (min-width: 1100px) {
  .fflex {
    width: 100%;
    display: -webkit-flex;
    -webkit-flex-flow: row;
    -webkit-align-items: stretch;
    -webkit-justify-content: space-between;
    display: flex;
    flex-flow: row;
    align-items: stretch;
    justify-content: space-between;
  }
}
#header_template .container,
#footer_template .container{
  margin: 0 auto;
  max-width: 1100px;
  padding: 0;
}
.fl {
  float: left;
  _display: inline;
}
.fr {
  float: right;
  _display: inline;
}
.sm-visible {
  display: none!important;
}
.sm-visible-inline {
  display: none!important;
}
.xs-visible {
  display: none!important;
}
.xs-visible-inline {
  display: none!important;
}
#greybg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: .6;
  z-index: 1000;
  display: none;
}

.header {
  background: #fff;
  height: 96px;
}
.header .row > .fr {
  padding-top: 26px;
}
.header .name {
  height: 30px;
  padding: 0 10px;
  border-radius: 50px;
  color: #fff;
  line-height: 30px;
  margin-top: 5px;
  background-image: -webkit-gradient(linear, 0 0, 100% 0, from(#4D41E3), to(#847EFF));
  background-image: -webkit-linear-gradient(left, #4D41E3, #847EFF);
  background-image: -moz-linear-gradient(left, #4D41E3, #847EFF);
  background-image: -o-linear-gradient(left, #4D41E3, #847EFF);
  background-image: linear-gradient(left, #4D41E3, #847EFF);
}
.header .name span {
  padding-left: 5px;
  color: #fff;
}
.header .a-links {
  padding-top: 5px;
  height: 30px;
  padding-left: 10px;
}
.header .a-links a {
  display: inline-block;
  line-height: 30px;
  color: #fff;
  padding: 0 10px;
  margin-right: 5px;
  float: left;
  background: #50B3FF;
}
.header .a-links a:nth-of-type(2) {
  background: #FF9C00;
}
.header .a-links a:nth-of-type(3) {
  background: #57C529;
}
.header .a-links a:nth-of-type(4) {
  background: #FF5656;
}
.logo {
  background: url(../images/logo.png) center no-repeat;
  height: 96px;
  width: 240px;
  text-indent: -9999px;
}
.slogen {
  background: url(../images/slogen.png) center no-repeat;
  width: 120px;
  height: 96px;
  text-indent: -9999px;
  margin-left: 10px;
}
.login-form {
  width: 406px;
  height: 40px;
  background: #FFAE44;
}
.login-form .loginbtn {
  width: 88px;
  height: 40px;
  text-align: center;
  background: #FF9000;
  line-height: 40px;
  font-size: 13px;
  color: #fff;
  cursor: pointer;
  border: 0;
  outline: 0;
  padding: 0;
}
.login-form .form {
  flex: 1;
}
.login-form .form > div {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 5px;
}
.login-form .form input {
  flex: 1;
  height: 30px;
  width: 100%;
  padding-left: 36px;
  border: 0;
  background: url(../images/icon/User.png) 8px center no-repeat #fff;
}
.login-form .form input[type="password"] {
  background-image: url(../images/icon/clcok.png);
}
.login-form .form .line {
  width: 10px;
}
.loginbtn-click {
  width: 88px;
  height: 40px;
  text-align: center;
  background: #FF9000;
  line-height: 40px;
  font-size: 13px;
  color: #fff;
  cursor: pointer;
}
@media only screen and (min-width: 768px) and (max-width: 1099px) {
