  }
  .xs-100 {
    width: 100%!important;
  }
  .xs-visible {
    display: block!important;
  }
  .xs-visible-inline {
    display: inline!important;
  }
  .xs-hidden {
    display: none!important;
  }
  .xs-text-right {
    text-align: right;
  }
  .xs-text-center {
    text-align: center;
  }
