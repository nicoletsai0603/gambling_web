    width: 100%!important;
  }
  .sm-visible {
    display: block!important;
  }
  .sm-visible-inline {
    display: inline!important;
  }
  .sm-hidden {
    display: none!important;
  }
  .sm-text-right {
    text-align: right;
  }
  .sm-text-center {
    text-align: center;
  }
  .sm-text-left {
    text-align: left;
  }
