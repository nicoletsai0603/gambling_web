}
.fix-right .qq:hover {
  color: #fff;
}
.fix-right div .qq:nth-of-type(2n+1) {
  background: #59C1FF;
}
.gototop {
  display: block;
  background: url(../images/icon/gototop.png) center no-repeat #BCBCBC;
  border-radius: 50%;
  width: 45px;
  height: 45px;
  margin: 15px auto;
}
.foot-links-box.row{
  margin: 0;
}
